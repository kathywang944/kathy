<view class="container">
  <view class="header">
    <view class="avatar-container">
      <image class="avatar" src="{{userInfo.avatarUrl}}" bindtap="navigateToRegister"></image>
      <text class="decoration-text">点击头像登录</text>
    </view>
  </view>

  <view class="info">
    <text class="info-text">救助的狗狗数量：{{userInfo.dogCount}}</text>
    <text class="info-text">加入云养日记的天数：{{userInfo.daysCount}}</text>
  </view>
</view>